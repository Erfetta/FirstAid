<%= form_with(model: @report, local: true) do |form| %>
  <%= form.label :report_time %>
  <%= form.time_select :report_time %>

  <%= form.label :report_date %>
  <%= form.date_select :report_date %>

  <%= form.label :coordinates %>
  <%= form.text_field :coordinates %>

  <%= form.hidden_field :user_id, value: 123456 %>

  <%= form.submit %>
<% end %>
